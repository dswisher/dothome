[
    {
        "key": "shift+alt+ctrl+cmd+d",
        "command": "workbench.action.debug.start",
        "when": "!inDebugMode"
    },
    {
        "key": "shift+alt+ctrl+cmd+r",
        "command": "workbench.action.debug.run",
        "when": "!inDebugMode"
    },
    {
        "key": "shift+alt+ctrl+cmd+o",
        "command": "outline.focus"
    },
    {
        "key": "shift+alt+ctrl+cmd+t",
        "command": "runCommands",
        "args": {
            "commands": [
                "testing.runAll",
                "workbench.view.testing.focus"
            ]
        }
    },
    {
        "key": "shift+alt+ctrl+cmd+g",
        "command": "workbench.action.tasks.runTask",
        "args": "Run Game"
    },
    {
        "key": "shift+alt+ctrl+cmd+u",
        "command": "workbench.action.tasks.runTask",
        "args": "Run Gui Test"
    }
]
